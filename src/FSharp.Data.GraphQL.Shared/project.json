{
  "version": "1.0.0",
  "buildOptions": {
    "compilerName": "fsc",
    "debugType": "portable",
    "emitEntryPoint": false,
    "compile": {
      "includeFiles": [
        "Extensions.fs",
        "Prolog.fs",
        "Ast.fs",
        "TypeSystem.fs",
        "Validation.fs",
        "Introspection.fs",
        "Parser.fs"
      ]
    },
    "define": [
      "DOTNETCORE",
      "TRACE"
    ],
    "nowarn": [
      "44",
      "2003"
    ],
    "xmlDoc": true,
    "delaySign": true,
    "warningsAsErrors": false,
    "additionalArguments": [
      "--fullpaths",
      "--flaterrors"
    ]
  },
  "dependencies": {
    "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-*",
    "FParsec": "1.0.2"
  },
  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-*",
      "imports": "dnxcore50"
    }
  },
  "frameworks": {
    "netstandard1.6": {
        "imports": "portable-net45+netcore45+wpa81+wp8"
    }
  }
}